[alias]
	allgraph = log --all --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(auto)%d%C(reset)'
	refgraph = log --reflog --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(auto)%d%C(reset)'
	po = push -u origin HEAD
[core]
	editor = vim
[help]
	autocorrect = 1
[include]
	path = ~/.gitconfig_local
