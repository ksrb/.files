<keymap version="1" name="Eclipse (Custom)" parent="Eclipse">
  <action id="CollapseAllRegions">
    <keyboard-shortcut first-keystroke="shift ctrl minus" />
  </action>
  <action id="ContextHelp" />
  <action id="ExpandAllRegions">
    <keyboard-shortcut first-keystroke="shift ctrl equals" />
  </action>
  <action id="GotoAction">
    <keyboard-shortcut first-keystroke="alt x" />
  </action>
  <action id="GotoFile">
    <keyboard-shortcut first-keystroke="ctrl p" />
  </action>
  <action id="NewElement">
    <keyboard-shortcut first-keystroke="alt insert" />
    <keyboard-shortcut first-keystroke="ctrl alt i" />
  </action>
  <action id="NextTab">
    <keyboard-shortcut first-keystroke="ctrl f6" />
    <keyboard-shortcut first-keystroke="ctrl page_down" />
    <keyboard-shortcut first-keystroke="ctrl alt l" />
  </action>
  <action id="PreviousTab">
    <keyboard-shortcut first-keystroke="shift ctrl f6" />
    <keyboard-shortcut first-keystroke="ctrl page_up" />
    <keyboard-shortcut first-keystroke="ctrl alt h" />
  </action>
  <action id="Print" />
  <action id="ShortcutLookup.ShortcutLookup">
    <keyboard-shortcut first-keystroke="ctrl alt k" />
  </action>
  <action id="ShowListAction">
    <keyboard-shortcut first-keystroke="ctrl alt k" />
  </action>
  <action id="ToggleDistractionFreeMode">
    <keyboard-shortcut first-keystroke="shift ctrl alt d" />
  </action>
  <action id="VimPluginToggle">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
  </action>
</keymap>