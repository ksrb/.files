Plug 'Shougo/neocomplcache.vim'

" Neocomplcache {
    let g:neocomplcache_enable_at_startup=1
    inoremap <expr><TAB> pumvisible() ? "\<C-n>" : "\<TAB>"
    inoremap <expr><S-TAB> pumvisible() ? "\<C-p>" : "\<S-TAB>"
" }

" Overrides {
  " UltiSnips {
      " if cygwin
      if has("win32unix")
        let g:UltiSnipsExpandTrigger="<C-@>"
      endif
  " }
" }
